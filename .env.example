# AK Unified API Configuration Example

# Logging
AKU_LOG_LEVEL=INFO
AKU_LOG_JSON=0
AKU_LOG_FORMAT={time} | {level} | {name}:{function}:{line} - {message}

# Database
AKU_DB_DSN=postgresql://user:password@localhost:5432/ak_unified
AKU_CACHE_TTL_SECONDS=3600
AKU_CACHE_TTL_PER_DATASET={"securities.equity.cn.ohlcv_daily": 86400, "macro.cn.cpi": 604800}

# Blob cache
AKU_BLOB_ALLOW_PREFIXES=securities,macro
AKU_BLOB_MAX_BYTES=104857600
AKU_BLOB_COMPRESS=1

# Rate limiting configuration
AKU_RATE_LIMIT_ENABLED=1

# Alpha Vantage rate limits (requests per minute/day)
AKU_AV_RATE_LIMIT_PER_MIN=5
AKU_AV_RATE_LIMIT_PER_DAY=500

# AkShare rate limits by vendor (requests per minute)
AKU_AKSHARE_EASTMONEY_RATE_LIMIT=60
AKU_AKSHARE_SINA_RATE_LIMIT=100
AKU_AKSHARE_TENCENT_RATE_LIMIT=80
AKU_AKSHARE_THS_RATE_LIMIT=30
AKU_AKSHARE_TDX_RATE_LIMIT=50
AKU_AKSHARE_BAIDU_RATE_LIMIT=40
AKU_AKSHARE_NETEASE_RATE_LIMIT=60
AKU_AKSHARE_HEXUN_RATE_LIMIT=30
AKU_AKSHARE_CSINDEX_RATE_LIMIT=20
AKU_AKSHARE_JISILU_RATE_LIMIT=10
AKU_AKSHARE_DEFAULT_RATE_LIMIT=30

# Vendors / adapters keys
AKU_ALPHAVANTAGE_API_KEY=your_alpha_vantage_api_key_here
AKU_IB_HOST=127.0.0.1
AKU_IB_PORT=7497
AKU_IB_CLIENT_ID=1

# Region mapping
AKU_REGION_MAPPING={"CN": "china", "US": "united_states", "HK": "hong_kong"}

# Normalization rules override
AKU_NORMALIZATION_RULES={"securities.equity.cn.ohlcv_daily": {"volume": "volume", "amount": "amount"}}